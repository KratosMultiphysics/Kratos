<?xml version="1.0" encoding="UTF-8"?>
<ElementList>
  <!--structural elements-->
  <!--beam elements-->
  <ElementItem n="BeamElement" pn="Small Displacement Beam" ImplementedInFile="small_displacement_beam_element_3D2N.cpp"
	       ImplementedInApplication="SolidMechanicsApplication" MinimumKratosVersion="9000" ProductionReady="ProductionReady"
	       WorkingSpaceDimension="3" LocalSpaceDimension="1" RequiresLocalAxes="True" LargeDeformation="False"
	       help="This element implements a small displacement timoshenko beam structural model"
		   ElementType="Beam" >

    <!--here we could add a list of all of the possible geometries-->
    <TopologyFeatures>
      <item GeometryType="Line" nodes="2" KratosName="SmallDisplacementBeamElement3D2N"/>
    </TopologyFeatures>
    
    <!-- here we add the block of features which we require from the constitutive law-->
    <ConstitutiveLaw_FilterFeatures>
      <filter field="Type" value="1D"/>
      <filter field="Dimension" value="3D"/>
      <filter field="StrainSize" value="6"/>
      <filter field="HybridType" value="D-R"/>
    </ConstitutiveLaw_FilterFeatures>
    
    <!--define list of NodalConditions-->
    <NodalConditions>
      <NodalCondition n="DISPLACEMENT" pn="Displacement" type="vector" fixity="Fix" analysis_type = "Static"  unit_magnitude="L" units="m"     reaction="REACTION"/>
      <NodalCondition n="ROTATION"     pn="Rotation"     type="vector" fixity="Fix" analysis_type = "Static"  unit_magnitude="Angle" units="rad" reaction="TORQUE"/>
      <NodalCondition n="VELOCITY"     pn="Velocity"     type="vector" fixity="Fix" analysis_type = "Dynamic" unit_magnitude="Velocity" units="m/s"     reaction=""/>
      <NodalCondition n="ACCELERATION" pn="Acceleration" type="vector" fixity="Fix" analysis_type = "Dynamic" unit_magnitude="Acceleration" units="m/s^2"     reaction=""/>
      <NodalCondition n="ANGULAR_VELOCITY"     pn="Angular Velocity"     type="vector" fixity="Fix" analysis_type = "Dynamic" unit_magnitude="Angle/T" units="rad/s"     reaction=""/>
      <NodalCondition n="ANGULAR_ACCELERATION" pn="Angular Acceleration" type="vector" fixity="Fix" analysis_type = "Dynamic" unit_magnitude="Angle/T^2" units="rad/s^2"    reaction=""/>
    </NodalConditions>

    <inputs>
      <parameter n="SECTION_TYPE" pn="Section type"/>
    </inputs>
    <outputs>
      <parameter n="GREEN_LAGRANGE_STRAIN_TENSOR" pn="Green-Lagrange strain tensor" />
      <parameter n="CAUCHY_STRESS_TENSOR" pn="Cauchy stress tensor"/>
    </outputs>
  </ElementItem>

