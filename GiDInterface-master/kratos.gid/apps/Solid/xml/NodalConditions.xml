		       analysis_type="Dynamic" unit_magnitude="Angle/T" units="rad/s" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
  </NodalConditionItem>
  
  <NodalConditionItem  n="RIGID_BODY_ANGULAR_ACCELERATION_3D" pn="Axis Angular Acceleration" ProcessName="ApplyRigidBodyRotationProcess3D" VariableName="ACCELERATION" kratos_module="SolidMechanicsApplication" v="No"
