  <NodalConditionItem  n="RIGID_BODY_ANGULAR_ACCELERATION_2D" pn="Axis Angular Acceleration" ProcessName="ApplyRigidBodyRotationProcess2D" VariableName="ACCELERATION" kratos_module="SolidMechanicsApplication" v="No"
