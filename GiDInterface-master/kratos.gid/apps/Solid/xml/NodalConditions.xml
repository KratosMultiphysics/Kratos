		      analysis_type="Static,Dynamic"  unit_magnitude="L" units="m" App="Solid" state="CheckNodalConditionStateSolid" Interval="Total">
    <outputs>
      <parameter n="REACTION" pn="Reaction" v="Yes"/>
    </outputs>
  </NodalConditionItem>
  
  <NodalConditionItem n="VELOCITY" pn="Velocity" ProcessName="AssignVectorComponentsToNodesProcess" VariableName="VELOCITY" kratos_module="SolidMechanicsApplication"
