  <NodalConditionItem  n="RIGID_BODY_ANGULAR_VELOCITY_2D" pn="Axis Angular Velocity" ProcessName="ApplyRigidBodyRotationProcess2D" VariableName="VELOCITY" kratos_module="SolidMechanicsApplication" v="No"
