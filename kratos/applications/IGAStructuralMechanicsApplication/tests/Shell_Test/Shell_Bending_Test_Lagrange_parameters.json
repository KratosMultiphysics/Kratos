    "rotation_dofs": "false"
  },
  "output_configuration": {
    "result_file_configuration": {
      "gidpost_flags": {
        "GiDPostMode": "GiD_PostAscii",
        "WriteDeformedMeshFlag": "WriteUndeformed",
        "WriteConditionsFlag": "WriteConditions",
        "MultiFileFlag": "SingleFile"
      },
      "file_label": "step",
      "output_control_type": "step",
