        "check_variables": [ "DISPLACEMENT_X", "DISPLACEMENT_Y", "DISPLACEMENT_Z" ],
        "input_file_name": "Shell_Test/Shell_Bending_Test_Lagrange_results.json",
        "model_part_name": "IGAModelPart",
        "time_frequency": 0.0000001
      }
    }
  ],
  "json_output_process": [
    {
      "python_module": "json_output_process",
      "kratos_module": "KratosMultiphysics",
      "help": "",
      "process_name": "JsonOutputProcess",
      "Parameters": {
        "output_variables": [ "DISPLACEMENT" ],
        "output_file_name": "Shell_Test/Shell_Bending_Test_Lagrange_results.json",
        "model_part_name": "IGAModelPart",
        "time_frequency": 0.0000001
      }
    }
  ]
