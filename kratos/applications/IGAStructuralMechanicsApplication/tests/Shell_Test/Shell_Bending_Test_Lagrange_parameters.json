{
  
  "problem_data": {
    "problem_name": "Shell_Test/TrussTestCases_kratos",
    "model_part_name": "IGAModelPart",
    "domain_size": 5,
    "start_time": 0,
    "end_time": 0,
    "time_step": 0.01
  },
  "Parameters": { "materials_filename": "Shell_Test/materials.json" },
  "solver_settings": {
    "solver_type": "iga_structural_mechanics_solver",
