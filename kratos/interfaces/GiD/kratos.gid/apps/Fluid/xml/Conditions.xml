<?xml version="1.0" encoding="UTF-8"?>
<ConditionList>
  <ConditionItem n="FaceCondition2D" pn="Face Condition 2D" ImplementedInFile="face_condition.cpp"
	       ImplementedInApplication="FluidDynamicsApplication" MinimumKratosVersion="9000" ProductionReady="ProductionReady"
	       WorkingSpaceDimension="3" LocalSpaceDimension="2" RequiresLocalAxes="False" LargeDeformation="False" MainDofsVariables="Velocity, Pressure"
	       ElementType="Line" help="This element implements a Face Condition to be used together with Fractional Step Element">

    <!--here we could add a list of all of the possible geometries-->
    <TopologyFeatures>
      <item GeometryOrder="linear" GeometryType="Line3D2" KratosName="FaceCondition2D3N"/>
    </TopologyFeatures>

    <!--define list of dofs-->
    <ConditionalConditions>
      <dof n="VELOCITY" pn="Velocity" type="vector" unit_magnitude="L" reaction="REACTION"/>
      <dof n="PRESSURE" pn="Pressure" type="vector" unit_magnitude="N/m^2" reaction="REACTION_AIR_PRESSURE"/>
    </ConditionalConditions>
    <inputs>
    </inputs>
    <outputs>
      <parameter n="VORTICITY" pn="VORTICITY" />
    </outputs>
  </ConditionItem>

  
  <ConditionItem n="FractionalStep3D" pn="Fractional Step 3D" ImplementedInFile="fractional_step.cpp"
	       ImplementedInApplication="FluidDynamicsApplication" MinimumKratosVersion="9000" ProductionReady="ProductionReady"
	       WorkingSpaceDimension="3" LocalSpaceDimension="3" RequiresLocalAxes="False" LargeDeformation="False" MainDofsVariables="Velocity, Pressure"
	       ElementType="Membrane" help="This element implements a Fractional Step Element">

    <!--here we could add a list of all of the possible geometries-->
    <TopologyFeatures>
      <item GeometryOrder="linear" GeometryType="Tetrahedra3D4" KratosName="FractionalStep3D4N"/>
    </TopologyFeatures>

    <!--define list of dofs-->
    <ConditionalConditions>
      <dof n="VELOCITY" pn="Velocity" type="vector" unit_magnitude="L" reaction="REACTION"/>
      <dof n="PRESSURE" pn="Pressure" type="vector" unit_magnitude="N/m^2" reaction="REACTION_AIR_PRESSURE"/>
    </ConditionalConditions>
    <inputs>
    </inputs>
    <outputs>
      <parameter n="VORTICITY" pn="VORTICITY" />
    </outputs>
  </ConditionItem>
  
</ConditionList>
