		  <value n="DeltaTime" pn="Delta time" v="0.001" help=""/>
		  <value n="TargetCFL" pn="Target CFL" v="5.0" help="" state="[CheckIdOnMyParent UseAutomaticDeltaTime Automatic]"/>
		  <value n="DivergenceCleareanceStep" pn="Divergence cleareance step" v="50" help="Define the divergence cleareance step value"/>
		  <value n="MaxVart" pn="Max Dt" v="0.0" help="Max time variation" state="[CheckIdOnMyParent UseAutomaticDeltaTime Automatic]"/>
		  <value n="MinVart" pn="Min Dt" v="0.0" help="Min time variation" state="[CheckIdOnMyParent UseAutomaticDeltaTime Automatic]"/>
