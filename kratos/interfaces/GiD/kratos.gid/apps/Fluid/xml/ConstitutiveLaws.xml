<?xml version="1.0" encoding="utf-8"?>
<ConstitutiveLaws>
    <!--linear elastic laws-->
	<CLaw n="Newtonian" pn="Newtonian" ProductionReady="ProductionReady" help="Newtonian fluid" >
        <inputs>
		  <parameter n="DENSITY" pn="Density" unit_magnitude="Density" units="kg/m^3" v="1000"/>
		  <parameter n="VISCOSITY" pn="Kinematic viscosity" unit_magnitude="L^2/T" units="m^2/s" v="1e-6" help="Set the kinematic viscosity."/>
        </inputs>
        <outputs>
        </outputs>
    </CLaw>
    <CLaw n="HerschelBulkley" pn="Herschel Bulkley" ProductionReady="ProductionReady" help="Herschel–Bulkley fluid -> tau = tau0 + k * gamma^n" >
        <inputs>
		  <parameter n="DENSITY" pn="Density" unit_magnitude="Density" units="kg/m^3" v="1000"/>
		  <parameter n="VISCOSITY" pn="Kinematic viscosity" unit_magnitude="L^2/T" units="m^2/s" v="1e-6" help="Fluidized viscosity."/>
		  <parameter n="YIELD_STRESS" pn="Yield stress (τ)" v="0" units="Pa" unit_magnitude="P"/>
		  <parameter n="POWER_LAW_K" pn="Consistency index (k)"  v="1"/>
		  <parameter n="POWER_LAW_N" pn="Flow index (n)" v="1" />
        </inputs>
        <outputs>
        </outputs>
    </CLaw>
</ConstitutiveLaws>
