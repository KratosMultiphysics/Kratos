<?xml version="1.0" encoding="UTF-8"?>
<NodalConditionList>
  <NodalConditionItem n="DISPLACEMENT" pn="Displacement" ProcessName="ImposeVectorValueByComponentsProcess" VariableName="DISPLACEMENT"
					  analysis_type="Static,Dynamic"  unit_magnitude="L" units="m">
    <outputs>
      <parameter n="REACTION" pn="Reaction" v="Yes"/>
    </outputs>
  </NodalConditionItem>
  
  <NodalConditionItem n="VELOCITY" pn="Velocity" ProcessName="ImposeVectorValueByComponentsProcess" VariableName="VELOCITY"
						analysis_type="Dynamic" unit_magnitude="Velocity" units="m/s">
  </NodalConditionItem>
  
  <NodalConditionItem  n="ACCELERATION" pn="Acceleration" ProcessName="ImposeVectorValueByComponentsProcess" VariableName="ACCELERATION"
						   analysis_type="Dynamic" unit_magnitude="Acceleration" units="m/s^2">
  </NodalConditionItem>
  
  <NodalConditionItem  n="PRESSURE" pn="Pressure" ProcessName="ImposeScalarValueProcess" VariableName="PRESSURE"
					   analysis_type = "Static,Dynamic"  unit_magnitude="P"  units="Pa" v="No" state="hidden" >
  </NodalConditionItem>
</NodalConditionList>
