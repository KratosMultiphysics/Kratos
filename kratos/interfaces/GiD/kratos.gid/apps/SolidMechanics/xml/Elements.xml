      <NodalCondition n="DISPLACEMENT" pn="Displacement" type="vector" analysis_type = "Static"  unit_magnitude="L" units="m" reaction="REACTION"/>
      <NodalCondition n="VELOCITY"     pn="Velocity"     type="vector" analysis_type = "Dynamic" unit_magnitude="Velocity" units="m/s" reaction=""/>
      <NodalCondition n="ACCELERATION" pn="Acceleration" type="vector" analysis_type = "Dynamic" unit_magnitude="Acceleration" units="m/s^2" reaction=""/>
    </NodalConditions>

    <inputs>      
      <parameter n="THICKNESS" pn="Thickness" unit_magnitude="L" units="m"  />
    </inputs>
    <outputs>
      <parameter n="GREEN_LAGRANGE_STRAIN_TENSOR" pn="Green-Lagrange strain tensor" />
      <parameter n="ALMANSI_STRAIN_TENSOR" pn="Almansi strain tensor"  />
      <parameter n="CAUCHY_STRESS_TENSOR" pn="Cauchy stress tensor" />
      <parameter n="PK2_STRESS_TENSOR" pn="Second Piola-Kirchhoff stress tensor"  />
