file	problemtype.bas
where	*# Nodal Variable blocks

Begin NodalData <NAME>
*loop nodes *OnlyInCond
*format "%i%10.5e"
*NodesNum 1 *cond(<NAME>)
*end nodes
End NodalData

file    problemtype.prb
where   # Properties

QUESTION: <NAME>
VALUE: <VALUE>

file    problemtype.tcl
where   # End Meshing Block

assigndefault <NAME> <NAME> <ENTITIES>

