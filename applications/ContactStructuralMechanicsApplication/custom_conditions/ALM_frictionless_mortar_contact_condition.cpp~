        lhs(0,12)=0;
        lhs(0,13)=0;
        lhs(0,14)=0;
        lhs(0,15)=0;
        lhs(0,16)=0;
        lhs(0,17)=0;
        lhs(0,18)=0;
        lhs(0,19)=0;
        lhs(0,20)=0;
        lhs(0,21)=0;
        lhs(0,22)=0;
        lhs(0,23)=0;
        lhs(0,24)=0;
        lhs(0,25)=0;
        lhs(0,26)=0;
        lhs(0,27)=0;
        lhs(1,0)=0;
        lhs(1,1)=0;
        lhs(1,2)=0;
        lhs(1,3)=0;
        lhs(1,4)=0;
        lhs(1,5)=0;
        lhs(1,6)=0;
        lhs(1,7)=0;
        lhs(1,8)=0;
        lhs(1,9)=0;
        lhs(1,10)=0;
