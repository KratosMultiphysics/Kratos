        rhs[22]=crhs39*crhs47 + crhs46*crhs8;
        rhs[23]=crhs39*crhs49 + crhs48*crhs8;
        rhs[24]=crhs32*scale_factor;
        rhs[25]=-crhs50*lmnormal[1];
        rhs[26]=crhs43*scale_factor;
        rhs[27]=-crhs50*lmnormal[3];
        }
