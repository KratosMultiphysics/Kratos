{
    "problem_data": {
        "problem_name": "gg",
        "parallel_type": "OpenMP",
        "echo_level": 0,
        "start_time": 0.0,
        "end_time": 1.0
    },
    "solver_settings": {
        "time_stepping": {
            "time_step": 0.25
        },
        "solver_type": "dynamic",
        "model_part_name": "Structure",
        "domain_size": 3,
        "echo_level": 0,
        "analysis_type": "linear",
        "model_import_settings": {
            "input_type": "use_input_model_part"
        },
        "material_import_settings": {
            "materials_filename": "auxiliary_files_7/StructuralMaterials.json"
        },
        "line_search": false,
        "convergence_criterion": "residual_criterion",
        "displacement_relative_tolerance": 0.0001,
        "displacement_absolute_tolerance": 1e-9,
        "residual_relative_tolerance": 0.0001,
        "residual_absolute_tolerance": 1e-9,
        "max_iteration": 10,
        "use_old_stiffness_in_first_iteration": false,
        "rotation_dofs": false,
        "volumetric_strain_dofs": false,
        "move_mesh_flag": false
    },
    "processes": {
        "constraints_process_list": [
            {
                "python_module": "assign_vector_variable_process",
                "kratos_module": "KratosMultiphysics",
                "help": "This process fixes the selected components of a given vector variable",
                "process_name": "AssignVectorVariableProcess",
                "Parameters": {
                    "model_part_name": "Structure.BoundPtsType7",
                    "variable_name": "DISPLACEMENT",
                    "value": [
                        0.0,
                        0.0,
                        0.0
                    ],
                    "constrained": [
                        true,
                        true,
                        true
                    ],
                    "interval": [
                        0.0,
                        "End"
                    ]
                }
            }
        ],
        "loads_process_list": [
            {
            "python_module" : "assign_vector_by_direction_to_condition_process",
            "kratos_module" : "KratosMultiphysics",
            "check"         : "DirectorVectorNonZero direction",
            "process_name"  : "AssignVectorByDirectionToConditionProcess",
            "Parameters"    : {
                "model_part_name" : "Structure.load_boundary_P2",
                "variable_name"   : "SURFACE_LOAD",
                "interval"        : [0.0,"End"],
                "modulus"         : "t*1000000.0",
                "direction"       : [1.0,0.0,0.0]
            }
        }
        ],
        "list_other_processes": []
    },
    "output_processes": {
        "hdf5_output": [
            
        ],        
        "sensor_output": [
            {
                "kratos_module": "KratosMultiphysics.SystemIdentificationApplication.processes",
                "python_module": "sensor_HDF5_output_process",
                "process_name": "SensorHDF5OutputProcess",
                "Parameters": {
                    "model_part_name": "Structure",
                    "sensor_model_part_name": "SensorModelPart",
                    "output_file_name": "auxiliary_files_7/hdf5_output/<model_part_name>_T_<step>.h5",
                    "sensor_value_variable_name": "SENSOR_COMPUTED_VALUE",
                    "@include_json": "auxiliary_files_7/sensor_data.json"
                }
            },
            {
                "kratos_module": "KratosMultiphysics.SystemIdentificationApplication.processes",
                "python_module": "primal_HDF5_output_process",
                "process_name": "PrimalHDF5OutputProcess",
                "Parameters": {
                    "model_part_name": "Structure",
                    "output_file_name": "auxiliary_files_7/hdf5_output/<model_part_name>_T_<step>.h5",
                    "nodal_solution_step_variables": ["DISPLACEMENT", "ACCELERATION"]
                }
            }
        ]
    },
    "analysis_stage": "KratosMultiphysics.StructuralMechanicsApplication.structural_mechanics_analysis"
}
