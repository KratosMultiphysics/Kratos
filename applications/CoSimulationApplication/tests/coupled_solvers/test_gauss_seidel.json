{
    "type" : "coupled_solvers.gauss_seidel",
    "settings" :
    {
        "timestep_start": 0,
        "delta_t": 0.01
    },
    "predictor" :
    {
        "type" : "predictors.linear"
    },
    "convergence_criterion" :
    {
        "type" : "convergence_criteria.or",
        "settings" :
        {
            "convergence_criterion0" :
            {
                "type": "convergence_criteria.iteration_limit",
                "settings":
                {
                    "maximum": 20
                }
            },
            "convergence_criterion1" :
            {
                "type" : "convergence_criteria.relative_norm",
                "settings" :
                {
                    "tolerance" : 1e-6,
                    "order" : 2
                }
            }
        }
    },
    "solver_wrappers" :
    [
        {
            "type" : "solver_wrappers.python.tube_flow_solver",
            "settings" :
            {
                "working_directory" : "coupled_solvers/CFD",
                "input_file"  : "solver_parameters.json",
                "interface_input" :
                {
                    "wall": "DISPLACEMENT"
                },
                "interface_output" :
                {
                    "wall" : ["PRESSURE","TRACTION"]
                }
            }
        },
        {
            "type" : "solver_wrappers.python.tube_structure_solver",
            "settings" :
            {
                "working_directory" : "coupled_solvers/CSM",
                "input_file"  : "solver_parameters.json",
                "interface_input" :
                {
                    "wall" : ["PRESSURE","TRACTION"]
                },
                "interface_output" :
                {
                    "wall": "DISPLACEMENT"
                }
            }
        }
    ]
}

