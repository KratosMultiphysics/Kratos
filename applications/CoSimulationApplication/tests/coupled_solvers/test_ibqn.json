{
    "type" : "coupled_solvers.ibqn",
    "settings" :
    {
        "timestep_start": 0,
        "delta_t": 0.01,
        "omega": 0.05,
        "relative_tolerance_gmres": 1e-12,
        "absolute_tolerance_gmres": 1e-18,
        "model_f":
        {
            "type": "coupled_solvers.models.ls",
            "settings":
            {
                "min_significant": 1e-12,
                "q": 10
            }
        },
        "model_s":
        {
            "type": "coupled_solvers.models.ls",
            "settings":
            {
                "min_significant": 1e-12,
                "q": 10
            }
        }
    },
    "predictor" :
    {
        "type" : "predictors.linear"
    },
    "convergence_criterion" :
    {
        "type" : "convergence_criteria.or",
        "settings" :
        {
            "convergence_criterion0" :
            {
                "type": "convergence_criteria.iteration_limit",
                "settings":
                {
                    "maximum": 20
                }
            },
            "convergence_criterion1" :
            {
                "type" : "convergence_criteria.relative_norm",
                "settings" :
                {
                    "tolerance" : 1e-6,
                    "order" : 2
                }
            }
        }
    },
    "solver_wrappers" :
    [
        {
            "type" : "solver_wrappers.pipe.flow_solver",
            "settings" :
            {
                "working_directory" : "coupled_solvers/CFD",
                "input_file"  : "solver_parameters.json",
                "interface_input" :
                {
                    "wall": "AREA"
                },
                "interface_output" :
                {
                    "wall" : "PRESSURE"
                }
            }
        },
        {
            "type" : "solver_wrappers.pipe.structure_solver",
            "settings" :
            {
                "working_directory" : "coupled_solvers/CSD",
                "input_file"  : "solver_parameters.json",
                "interface_input" :
                {
                    "wall" : "PRESSURE"
                },
                "interface_output" :
                {
                    "wall": "AREA"
                }
            }
        }
    ]
}



