{
    "problem_data": {
        "problem_name": "geometry",
        "parallel_type": "<PARALLEL_TYPE>",
        "echo_level": 0,
        "start_time": 0.0,
        "end_time": 0.05
    },
    "output_processes": {},
    "solver_settings": {
        "model_part_name": "FluidModelPart",
        "domain_size": 2,
        "solver_type": "CoupledRANS",
        "model_import_settings": {
            "input_type": "mdpa",
            "input_filename": "backward_facing_step",
            "partition_in_memory": true
        },
        "echo_level": -1,
        "volume_model_part_name": "Parts_fluid",
        "skin_parts": [
            "AutomaticInlet2D_inlet",
            "Outlet2D_outlet",
            "Slip2D"
        ],
        "time_stepping": {
            "automatic_time_step": false,
            "time_step": 0.01
        },
        "time_scheme_settings": {
            "scheme_type": "<TIME_SCHEME_TYPE>"
        },
        "material_import_settings": {
            "materials_filename": "backward_facing_step_material_properties.json"
        },
        "no_skin_parts": [],
        "formulation_settings": {
            "formulation_name": "fractional_step",
            "predictor_corrector": false,
            "maximum_velocity_iterations": 20,
            "maximum_pressure_iterations": 1,
            "velocity_tolerance": 1e-4,
            "pressure_tolerance": 1e-3,
            "dynamic_tau": 0.01,
            "oss_switch": 0,
            "echo_level": 0,
            "time_order": 2,
            "compute_reactions": false,
            "reform_dofs_at_each_step": false,
            "pressure_linear_solver_settings": {
                "solver_type": "amgcl",
                "tolerance": 1e-12
            },
            "velocity_linear_solver_settings": {
                "solver_type": "amgcl",
                "tolerance": 1e-12
            }
        }
    },
    "processes": {
        "initial_conditions_process_list": [
            {
                "python_module": "process_factory",
                "kratos_module": "KratosMultiphysics",
                "process_name": "ApplyConstantScalarValueProcess",
                "Parameters": {
                    "model_part_name": "FluidModelPart",
                    "variable_name": "VISCOSITY",
                    "value": 1.0
                }
            },
            {
                "python_module": "process_factory",
                "kratos_module": "KratosMultiphysics",
                "process_name": "ApplyConstantVectorValueProcess",
                "Parameters": {
                    "model_part_name": "FluidModelPart",
                    "variable_name": "VELOCITY",
                    "modulus": 0.0,
                    "direction": [
                        1.0,
                        0.0,
                        0.0
                    ]
                }
            },
            {
                "python_module": "process_factory",
                "kratos_module": "KratosMultiphysics",
                "process_name": "ApplyConstantScalarValueProcess",
                "Parameters": {
                    "model_part_name": "FluidModelPart",
                    "variable_name": "DENSITY",
                    "value": 1.0
                }
            }
        ],
        "boundary_conditions_process_list": [
            {
                "python_module": "apply_inlet_process",
                "kratos_module": "KratosMultiphysics.FluidDynamicsApplication",
                "Parameters": {
                    "model_part_name": "FluidModelPart.AutomaticInlet2D_inlet",
                    "variable_name": "VELOCITY",
                    "modulus": "1.0 * t / 3.0",
                    "direction": "automatic_inwards_normal",
                    "interval": [
                        0.0,
                        "End"
                    ]
                }
            },
            {
                "python_module": "apply_outlet_process",
                "kratos_module": "KratosMultiphysics.FluidDynamicsApplication",
                "Parameters": {
                    "model_part_name": "FluidModelPart.Outlet2D_outlet",
                    "variable_name": "PRESSURE",
                    "constrained": true,
                    "value": 0.0,
                    "hydrostatic_outlet": false,
                    "h_top": 0.0
                }
            },
            {
                "python_module": "apply_wall_function_process",
                "kratos_module": "KratosMultiphysics.RANSApplication",
                "Parameters": {
                    "model_part_name": "FluidModelPart.Slip2D"
                }
            }
        ],
        "gravity": [
            {
                "python_module": "assign_vector_by_direction_process",
                "kratos_module": "KratosMultiphysics",
                "process_name": "AssignVectorByDirectionProcess",
                "Parameters": {
                    "model_part_name": "FluidModelPart.Parts_fluid",
                    "variable_name": "BODY_FORCE",
                    "modulus": 0.0,
                    "constrained": false,
                    "direction": [
                        1.0,
                        0.0,
                        0.0
                    ]
                }
            }
        ],
        "auxiliar_process_list": [
            {
                "python_module": "from_json_check_result_process",
                "kratos_module": "KratosMultiphysics",
                "help": "",
                "process_name": "FromJsonCheckResultProcess",
                "Parameters": {
                    "check_variables": [
                            "VELOCITY_X",
                            "VELOCITY_Y",
                            "PRESSURE"
                    ],
                    "input_file_name": "backward_facing_step_fs_up_field_<TIME_SCHEME_TYPE>_results.json",
                    "model_part_name": "FluidModelPart.Parts_fluid",
                    "tolerance": 1e-9,
                    "relative_tolerance": 1e-12,
                    "time_frequency": -2
                }
            }
        ]
    },
    "_print_output_process_list": [
        {
            "python_module": "from_json_check_result_process",
            "kratos_module": "KratosMultiphysics",
            "help": "",
            "process_name": "FromJsonCheckResultProcess",
            "Parameters": {
                "check_variables": [
                        "VELOCITY_X",
                        "VELOCITY_Y",
                        "PRESSURE"
                ],
                "input_file_name": "backward_facing_step_fs_up_field_<TIME_SCHEME_TYPE>_results.json",
                "model_part_name": "FluidModelPart.Parts_fluid",
                "tolerance": 1e-9,
                "relative_tolerance": 1e-12,
                "time_frequency": -2
            }
        },
        {
            "python_module": "json_output_process",
            "kratos_module": "KratosMultiphysics",
            "help": "",
            "process_name": "JsonOutputProcess",
            "Parameters": {
                "output_variables": [
                        "VELOCITY_X",
                        "VELOCITY_Y",
                        "PRESSURE"
                ],
                "output_file_name": "backward_facing_step_fs_up_field_<TIME_SCHEME_TYPE>_results.json",
                "model_part_name": "FluidModelPart.Parts_fluid",
                "time_frequency": -2
            }
        }
    ]
}